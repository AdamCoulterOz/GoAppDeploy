{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "dbAdminUser": {
            "value": "dbuser"
        },
        "dbCores": {
            "value": 1
        },
        "dbSizeGB": {
            "value": 5
        },
        "dbPlanTier": {
            "value": "Basic"
        },
        "postgreSqlVersion": {
            "value": "10"
        },
        "dbName": {
          "value": "app"
        },
        "dbCollation": {
            "value": "en_US.utf8"
        },
        "dbCharset": {
            "value": "utf8"
        },
        "siteAction":{
            "value": "updatedb -s"
        },
        "webPlanTier": {
            "value": "Free"
        },
        "webPlanCode": {
            "value": "F1"
        }
    }
}
